{
  "manifest_version": 2,
  "name": "Useless Ideas",
  "description": "Useless Ideas New Tab",
  "version": "0.2",
  "permissions": [
    "activeTab",
    "<all_urls>",
    "*://*/*"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "chrome_url_overrides": {
    "newtab": "override.html"
  }
}
